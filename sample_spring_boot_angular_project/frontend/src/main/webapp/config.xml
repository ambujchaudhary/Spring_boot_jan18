        <splash height="480" src="mobile/resources/ios/splash/Default~iphone.png" width="320" />
        <splash height="2732" src="mobile/resources/ios/splash/Default@2x~universal~anyany.png" width="2732" />
        <edit-config file="*-Info.plist" mode="merge" overwrite="true" target="NSPhotoLibraryUsageDescription">
            <string>Shootzu app requires access to your photo library for the upload of portfolio images in your profile.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" overwrite="true" target="NSCameraUsageDescription">
            <string>Shootzu app requires access to your camera so you have the option to take a profile image.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" overwrite="true" target="NSMicrophoneUsageDescription">
            <string>Shootzu app requires access to the microphone to create and upload video files to your portfolio.</string>
        </edit-config>
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^2.2.0" />
