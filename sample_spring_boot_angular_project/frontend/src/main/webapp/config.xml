<?xml version='1.0' encoding='utf-8'?>
<widget id="com.shootzu.shootzu" version="1.2.41" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Shootzu</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <preference name="orientation" value="portrait" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="OverrideUserAgent" value="Mozilla/5.0 Google" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="mobile/resources/android/icon/Icon-36.png" />
        <icon density="mdpi" src="mobile/resources/android/icon/Icon-48.png" />
        <icon density="hdpi" src="mobile/resources/android/icon/Icon-72.png" />
        <icon density="xhdpi" src="mobile/resources/android/icon/Icon-96.png" />
        <icon density="xxhdpi" src="mobile/resources/android/icon/Icon-144.png" />
        <icon density="xxxhdpi" src="mobile/resources/android/icon/Icon-192.png" />
        <splash density="land-ldpi" src="mobile/resources/android/splash/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="mobile/resources/android/splash/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="mobile/resources/android/splash/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="mobile/resources/android/splash/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="mobile/resources/android/splash/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="mobile/resources/android/splash/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="mobile/resources/android/splash/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="mobile/resources/android/splash/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="mobile/resources/android/splash/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="mobile/resources/android/splash/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="mobile/resources/android/splash/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="mobile/resources/android/splash/drawable-port-xxxhdpi-screen.png" />
        <config-file parent="/resources" target="./res/values/strings.xml">
            <string name="fb_app_id">196099597970374</string>
            <string name="fb_app_name">com.shootzu.shootzu</string>
        </config-file>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="mobile/resources/ios/icon/icon.png" width="57" />
        <icon height="114" src="mobile/resources/ios/icon/icon@2x.png" width="114" />
