{
  "$schema": "./node_modules/@angular-devkit/core/src/workspace/workspace-schema.json",
  "version": 1,
  "defaultProject": "shootzu",
  "projects": {
    "app": {
      "root": "",
      "sourceRoot": "mobile/src",
      "projectType": "application",
      "prefix": "zum",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "progress": false,
            "outputPath": "www",
            "index": "mobile/src/index.html",
            "main": "mobile/src/main.ts",
            "polyfills": "mobile/src/polyfills.ts",
            "tsConfig": "mobile/src/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "mobile/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/@ionic/angular/dist/ionic/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              {
                "input": "./mobile/src/global.scss"
              },
              {
                "input": "./web/src/styles.scss"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.mobile.prod.ts"
                },
                {
                  "replace": "mobile/src/index.html",
                  "with": "mobile/src/index.prod.html"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.mobile.dev.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "roman": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.mobile.roman.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "roman-mural": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.mobile.roman_mural.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "roman-server": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.mobile.roman_server.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "pavlo": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.mobile.pavlo.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "petro": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.mobile.petro.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build",
            "proxyConfig": "mobile/proxy.conf.json",
            "port": 8100,
            "host": "0.0.0.0"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "dev": {
              "browserTarget": "app:build:dev"
            },
            "roman": {
              "browserTarget": "app:build:roman"
            },
            "roman-mural": {
              "browserTarget": "app:build:roman-mural"
            },
            "roman-server": {
              "browserTarget": "app:build:roman-server"
            },
            "pavlo": {
              "browserTarget": "app:build:pavlo"
            },
            "petro": {
              "browserTarget": "app:build:petro"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "mobile/src/test.ts",
            "polyfills": "mobile/src/polyfills.ts",
            "tsConfig": "mobile/src/tsconfig.spec.json",
            "karmaConfig": "mobile/src/karma.conf.js",
            "styles": [
              "web/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "mobile/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "mobile/src/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "mobile/src/tsconfig.app.json",
              "mobile/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/ng-toolkit:cordova-build",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "dev": {
              "browserTarget": "app:build:dev"
            },
            "roman": {
              "browserTarget": "app:build:roman"
            },
            "roman-mural": {
              "browserTarget": "app:build:roman-mural"
            },
            "roman-server": {
              "browserTarget": "app:build:roman-server"
            },
            "pavlo": {
              "browserTarget": "app:build:pavlo"
            },
            "petro": {
              "browserTarget": "app:build:petro"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/ng-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "app:ionic-cordova-build",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "app:ionic-cordova-build:production",
              "devServerTarget": "app:serve:production"
            },
            "dev": {
              "cordovaBuildTarget": "app:ionic-cordova-build:dev",
              "devServerTarget": "app:serve:dev"
            },
            "roman": {
              "cordovaBuildTarget": "app:ionic-cordova-build:roman",
              "devServerTarget": "app:serve:roman"
            },
            "roman-mural": {
              "cordovaBuildTarget": "app:ionic-cordova-build:roman-mural",
              "devServerTarget": "app:serve:roman-mural"
            },
            "roman-server": {
              "cordovaBuildTarget": "app:ionic-cordova-build:roman-server",
              "devServerTarget": "app:serve:roman-server"
            },
            "pavlo": {
              "cordovaBuildTarget": "app:ionic-cordova-build:pavlo",
              "devServerTarget": "app:serve:pavlo"
            },
            "petro": {
              "cordovaBuildTarget": "app:ionic-cordova-build:petro",
              "devServerTarget": "app:serve:petro"
            }
          }
        }
      }
    },
    "app-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "app:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "shootzu": {
      "root": "",
      "sourceRoot": "web/src/",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../../../../backend/src/main/resources/static",
            "index": "web/src/index.html",
            "main": "web/src/main.ts",
            "tsConfig": "web/src/tsconfig.app.json",
            "polyfills": "web/src/polyfills.ts",
            "assets": [
              "web/src/assets",
              "web/src/favicon.ico"
            ],
            "styles": [
              "web/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                },
                {
                  "replace": "web/src/index.html",
                  "with": "web/src/index.prod.html"
                }
              ]
            },
            "dev": {
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.dev.ts"
                },
                {
                  "replace": "web/src/index.html",
                  "with": "web/src/index.dev.html"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "shootzu:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "shootzu:build:production"
            },
            "dev": {
              "browserTarget": "shootzu:build:dev"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shootzu:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "web/src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "web/src/polyfills.ts",
            "tsConfig": "web/src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "web/src/styles.scss"
            ],
            "assets": [
              "web/src/assets",
              "web/src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "web/src/tsconfig.app.json",
              "web/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "shootzu-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "shootzu:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@ionic/schematics-angular"
  },
  "schematics": {
    "@ionic/schematics-angular:component": {
      "prefix": "zum",
      "styleext": "scss",
      "colors": true
    },
    "@ionic/schematics-angular:page": {
      "prefix": "zum",
      "styleext": "scss",
      "colors": true
    },
    "@schematics/angular:component": {
      "prefix": "zu",
      "styleext": "scss",
      "colors": true
    },
    "@schematics/angular:directive": {
      "prefix": "zu",
      "colors": true
    }
  }
}
