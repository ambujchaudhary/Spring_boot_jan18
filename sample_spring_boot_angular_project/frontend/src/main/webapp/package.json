  "scripts": {
    "ng": "ng",
    "serve": "ng serve --configuration=dev --proxy-config proxy.conf.json",
    "start": "ng serve --proxy-config proxy.conf.json --open",
    "build": "ng build --prod --configuration=dev",
    "build-prod": "ng build --prod --configuration=production",
    "build-dev": "ng build --configuration=dev",
    "test": "ng test --code-coverage",
    "test-with-coverage": "ng test --source-map=true --code-coverage",
    "test-coverage-not_sm": "ng test --source-map=false --code-coverage",
    "lint": "ng lint",
    "bundle-report": "webpack-bundle-analyzer ./../../../../backend/src/main/resources/static/stats.json",
    "iserve": "ionic serve --configuration=roman --no-open",
    "irelease": "ionic cordova build --configuration=production --prod --release",
    "irelease-dev": "ionic cordova build --configuration=dev --prod --release",
    "iserve-lab": "ionic serve --configuration=roman -l --no-open",
    "ibuild": "ionic cordova build --configuration=roman --platform -- --buildFlag='-UseModernBuildSystem=0'",
    "ibuild-dev": "ionic cordova build --configuration=dev --platform -- --buildFlag='-UseModernBuildSystem=0'",
    "ibuild-prod": "ionic cordova build --configuration=prod --platform -- --buildFlag='-UseModernBuildSystem=0'",
    "ibuild-local-roman": "ionic cordova build --configuration=roman --platform -- --buildFlag='-UseModernBuildSystem=0'",
    "irun-android-local-roman": "ionic cordova run --configuration=roman android --device --verbose",
    "ibuild-server-roman": "ionic cordova build --configuration=roman-server --platform -- --buildFlag='-UseModernBuildSystem=0'",
    "irun-android-server-roman": "ionic cordova run --configuration=roman-server android --device --verbose",
    "ibuild-local-petro": "ionic cordova build --configuration=petro --platform -- --buildFlag='-UseModernBuildSystem=0'",
    "irun-android-local-petro": "ionic cordova run --configuration=petro android --device --verbose",
    "gulp": "gulp"
  },
